                </Attr>
                <Attr>
                    <Arg>"dx"</Arg>
                    <Arg>
                        var arrowLength = Math.max($strokeStyle.w * 4, 6) * 2;
                        var dx = 0;
                        
                        dx;
                    </Arg>
                </Attr>
            </For>
        </Behaviors>  
